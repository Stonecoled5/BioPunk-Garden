go.property("plot_instance", hash("none"))

function init(self)

	go.set_scale(1.5)
	
	if self.plot_instance ~= nil then
		sprite.play_flipbook("#sprite", self.plot_instance)
	else
		msg.post("#sprite", "disable")
	end
end

